procedure clamp(value: real, min: real, max: real): real {
    check (value > max) {
        return max;
    } otherwise check (value < min) {
        return min;
    } otherwise {
        return value;
    }
}

procedure takeoff(): void {
    varying value: int = 123;
    varying clamped: int = clamp(value, 100, 110);
    printf("The value is: " + clamped);
    return;
}